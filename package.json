{
  "private": false,
  "name": "webnd-project5-travel-app",
  "version": "1.0.0",
  "description": "Travel App - Project 5 for the Front End Web Developer Nanodegree at Udacity ",
  "license": "CC0-1.0",
  "author": "Luca Borrione",
  "homepage": "https://github.com/luca-borrione/webnd-project5-travel-app",
  "repository": {
    "type": "git",
    "url": "git@github.com:luca-borrione/webnd-project5-travel-app.git"
  },
  "scripts": {
    "check-yarn-integrity": "yarn check --integrity",
    "clean": "rm -rf dist",
    "compile": "webpack --config webpack.prod.js",
    "compile:dev": "webpack --config webpack.dev.js",
    "dev": "yarn clean && webpack-dev-server --config webpack.dev.js --open --port=3000",
    "format": "npm-run-all --parallel format:*",
    "format:html": "prettier --write 'src/client/views/**/*.html'",
    "format:js": "$npm_execpath run lint:js --fix",
    "format:json": "prettier --write '*.json'",
    "format:scss": "$npm_execpath run lint:scss --fix",
    "lint": "npm-run-all --parallel lint:*",
    "lint:html": "linthtml 'src/client/views/**/*.html'",
    "lint:js": "eslint . --ext .js",
    "lint:scss": "stylelint 'src/client/styles/**/*.scss'",
    "prepare": "husky install",
    "start": "node src/server/index.js",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "~1.19.0",
    "cors": "~2.8.5",
    "dotenv": "~10.0.0",
    "express": "~4.17.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "node-fetch": "~2.6.6",
    "terser-webpack-plugin": "^5.2.5"
  },
  "devDependencies": {
    "@babel/core": "~7.16.0",
    "@babel/plugin-transform-runtime": "~7.16.4",
    "@babel/preset-env": "~7.16.4",
    "@linthtml/linthtml": "~0.8.0",
    "@linthtml/linthtml-config-recommended": "~0.1.0",
    "@types/jest": "^27.0.3",
    "babel-loader": "~8.2.3",
    "babel-plugin-transform-async-to-promises": "~0.8.16",
    "clean-webpack-plugin": "~4.0.0",
    "css-loader": "~6.5.1",
    "css-minimizer-webpack-plugin": "~3.1.4",
    "eslint": "~8.2.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "husky": "~7.0.4",
    "jest": "~26.6.3",
    "lint-staged": "~12.0.3",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "~6.0.1",
    "npm-run-all": "~4.1.5",
    "postcss": "~8.3.11",
    "prettier": "~2.4.1",
    "prettier-package-json": "~2.6.0",
    "prettier-plugin-sort-json": "~0.0.2",
    "regenerator-runtime": "^0.13.9",
    "sass": "~1.43.4",
    "sass-loader": "~12.3.0",
    "style-loader": "~3.3.1",
    "stylelint": "~14.1.0",
    "stylelint-config-prettier": "~9.0.3",
    "stylelint-config-standard": "~24.0.0",
    "stylelint-config-standard-scss": "~2.0.1",
    "supertest": "^6.1.6",
    "typescript": "^4.5.2",
    "webpack": "~5.64.2",
    "webpack-bundle-analyzer": "~4.5.0",
    "webpack-cli": "~4.9.1",
    "webpack-dev-server": "~4.5.0"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "~8.1.0",
    "yarn": "~1.22.17"
  }
}
